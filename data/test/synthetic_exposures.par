[TARGET]
band = 'H'
FoV = 22*u.arcsec
shape = (1024, 1024)
star_table = data/test/example_star_table.dat
sky_background = 14.4

[TELESCOPE]
diameter = 8.2*u.m
central_obscuration = 0.14
name = VLT Unit Telescope
